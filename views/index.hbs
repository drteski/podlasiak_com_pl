<main class='main-container'>
	<section class='features-container' id='features'>
		<h2 class='about about--other'>{{#if
				translations.layout.features.title}}{{translations.layout.features.title}}{{else}}
			translations.layout.features.title{{/if}}</h2>
		<div class='features-cont'>
			{{#each translations.layout.features.content}}
				<div class='feature-box'>
					<img src="{{this.img}}" alt="" class="feature__img">
					<h4 class='feature__name'>{{#if this.title}}{{this.title}}{{else}}
						translations.layout.features.content.title{{/if}}</h4>
					<p class='feature__desc'>{{#if this.desc}}{{this.desc}}{{else}}
						translations.layout.features.content.title{{/if}}</p>
				</div>
			{{/each}}
		</div>
	</section>
	<section class='reviews-container' id='reviews'>
		<h3 class='about about--other'>{{#if
				translations.layout.reviews.title}}{{translations.layout.reviews.title}}{{else}}
			translations.layout.reviews.title{{/if}}</h3>
		<div class='reviews'>
			{{#each translations.layout.reviews.review}}
				<div class='reviews-box'>
					<div class='review'>
						<div class='comment'>
							<p class='comment__text'>
								<strong class='comment__text comment__text--quot'>„</strong>{{#if
									this.desc}}{{this.desc}}{{else}}this.desc{{/if}}
								<strong
										class='comment__text comment__text--quot'>"</strong>
							</p>
						</div>
					</div>
					<div class='client'>
						<p class='client__name'>{{#if this.user}}{{this.user}}{{else}}
							this.user{{/if}}</p>
						<img alt='Logo' class='client__logo'
								src='/images/opinieo.png'>
					</div>
				</div>
			{{/each}}
		</div>
		<div class='grade-box'>
			<div class='grade'>
				<div class='left-side'>
					<img alt='Logo' class='grade__logo'
							src='http://podlasiak.com.pl/imgs/opinieo.png'>
					<div class='grade__text'>
						<div class='grade__box'>
							<h3 class='text-grade'><strong>4.8</strong> / 5</h3>
							<div class='percent'>
								<img alt='Thumb' class='percent__hand'
										src='http://podlasiak.com.pl/imgs/thumb_positive.png'>
								<p class='percent__text'>97%</p>
							</div>
							<p class='desc-grade'>{{#if
									translations.layout.reviews.desc.recommended}}{{translations.layout.reviews.desc.recommended}}{{else}}
								translations.layout.reviews.desc.recommended{{/if}}</p>
						</div>
						<img alt='Quality' class='quality'
								src='http://podlasiak.com.pl/imgs/opineo-quality.png'>
					</div>
				</div>
				<div class='review-text'>
					<p class='review-text__top'>{{#if
							translations.layout.reviews.desc.firstPart}}{{translations.layout.reviews.desc.firstPart}}{{else}}
						translations.layout.reviews.desc.firstPart{{/if}}
					</p>
					<p class='review-text__bottom'>{{#if
							translations.layout.reviews.desc.secondPart}}{{translations.layout.reviews.desc.secondPart}}{{else}}
						translations.layout.reviews.desc.secondPart{{/if}}</p>
				</div>
			</div>
		</div>
	</section>
	<section class='about-container' id='about'>
		{{>europe_svg}}
		<div class='text-wrap'>
			<div class='text-cont'>
				<h3 class='about about--other'>{{#if
						translations.layout.about.title}}{{translations.layout.about.title}}{{else}}
